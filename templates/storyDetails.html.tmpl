<div class="story-item">
<!--<a class="upvote-button" href="https://news.ycombinator.com/item?id={{.ID}}" target="_blank" rel="noopener noreferrer">▲</a>//-->
<div>
  <div>
    <a class="story-title" href="{{.URL}}">{{.Title}}</a>
    {{if ne .Domain ""}} <span class="story-domain">(<a href="https://news.ycombinator.com/from?site={{.Domain}}">{{.Domain}}</a>)</span>{{end}}
  </div>
  <div class="story-details">
    {{if .Flagged}}[flagged]{{end}}
    {{if .Dupe}}[dupe]{{end}}
    <a href="/stats?id={{.ID}}">
      <span 
        title="Estimated True Upvote Rate:&#013;Ratio of how more or less likely users are to upvote this story compared to the average story."
        {{if ge .QualityString "1"}} class="quality-string" {{end}}
        {{if ge .QualityString "2"}} style="font-weight:bold;" {{end}}>
        ×{{.QualityString}}</span> 

      {{if ne .RankDiff 0}}
        <span class={{if lt .RankDiff 0}}"penalty"{{else}}"boost"{{end}} title="Rank Delta:&#013;Difference between story's actual rank and the raw rank based on the HN ranking formula.">
          {{if gt .RankDiff 0}}+{{end}}{{.RankDiff}}
        </span>
      {{end}}
    </a>
    <span class="comparative-rank">
      {{if and .TopRank.Valid (not .IsHNTopPage)}}<a href="/#{{.ID}}"><span title="Rank on Hacker News Front Page" class="rank-icon hn">#{{.TopRank.Value}}</span></a> {{end}}

{{/*      {{if and .QNRank.Valid (le .QNRank.Int32 90) (not .IsQualityPage) }}<a href="/#{{.ID}}"><span class="rank-icon qn">#{{.QNRank.Value}}</span> on QN</a> |{{end}} */}}
    </span>
    | 
    {{.Score}} points by <a href="https://news.ycombinator.com/user?id={{.By}}">{{.By}}</a> 
    {{if .IsResubmitted}}<span class="original-age" title="Original submission time: {{.OriginalISOTimestamp}}">{{.OriginalAgeString}}</span>{{end}} 
    <a href="https://news.ycombinator.com/item?id={{.ID}}" title="2nd-Chance submission time: {{.ISOTimestamp}}"{{if .IsResubmitted}} class="resubmitted-age"{{end}}>{{.AgeString}}</a> |
    <a href="https://news.ycombinator.com/item?id={{.ID}}">{{if (eq .Comments 0)}}discuss{{else}}{{.Comments}}&nbsp;comments{{end}}</a> 

  </div>
</div>
</div>
