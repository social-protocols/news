body {
  background-color: #F6F6EF;
  font-size:13px;
  color:#828282;
  font-family:Verdana, Geneva, sans-serif;
  margin: 0;
}

.story-item {
  display: flex;
  align-items: baseline;
}

a:link.upvote-button, a:visited.upvote-button {
  color: #9A9A9A;
  margin-right: 0.5rem;
  font-size: 13px;
  align-self: flex-start;
  flex-shrink: 0;
  flex-grow: 0;
  width: 15px;
}

.story-title {
  font-size: 15px;
}

.story-details {
  font-size: 12px;
  color: #828282;
}

.quality-string {
  color: #0089f4;
  text-align: left;
}


.rank-icon {
  display: inline-block;
  text-align: center;
  min-width: 2em;
  padding-left:  2px;
  padding-right: 3px;
  padding-top: 1px;
  padding-bottom: 1px;
  border-radius:  6px;
  background-color:  #d0d0e0;

}

.stories li {
  margin-bottom: 6px;
}

.stories li::marker {
  font-size: 15px;
  color: #828282;
}

.stories {
  margin-left:  5px;
  margin-right:  7px;
}

a:link    { color:#000000; text-decoration:none; }
a:visited { color:#828282; text-decoration:none; }
.story-details a:link { color:#828282; text-decoration:none; }

.header {
  background-color: #FF6600;
  color: #000000;
  font-size: 12px;
  padding: 6px 10px;
  display:flex;
  align-items: center;
}

.header a:link, .header a:visited {
  color: #000000;
}

a:link.header-title, a:visited.header-title {
  margin-left: 0.5rem;
  margin-right: 1rem;
  font-weight: bold;
  font-size: 15px;
}

.icon {
  width: 20px;
  height: 20px;
}

.stats {
  margin-left:  5px;
  margin-right:  7px;
}

</style>
<title>Quality News</title>
</head>
